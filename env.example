# Server Configuration
NODE_ENV=development
PORT=3000
HOST=0.0.0.0

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Redis (Upstash)
REDIS_URL=rediss://your-redis-url
REDIS_TOKEN=your-redis-token

# AI Providers
ANTHROPIC_API_KEY=sk-ant-your-key
OPENAI_API_KEY=sk-your-key
TAVILY_API_KEY=tvly-your-key

# LangSmith (Optional - for debugging/tracing)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=
LANGCHAIN_PROJECT=marketerx-chat

# Cache TTLs (seconds)
CACHE_TTL_SESSION=1800
CACHE_TTL_KNOWLEDGE=86400
CACHE_TTL_CANON=604800

# Rate Limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=60000

# AI Models (Claude 4.5 series - 2026)
TIER1_MODEL=claude-sonnet-4-20250514
TIER2_MODEL=claude-haiku-4-20251015
TIER2_MODEL_ALT=gpt-4o-mini
EMBEDDING_MODEL=text-embedding-3-large


